   "source": [
    "class FourCal:\n",
    "    def setdata(self,first=0,second=0): #매개변수/지역변수\n",
    "        self.first = first #인스턴스 변수\n",
    "        self.second = second #인스턴스 변수\n",
    "        \n",
    "    def add(self):\n",
    "        result = self.first + self.second\n",
    "        return result\n",
    "    \n",
    "    def __init__(self,first=0,second=0):\n",
    "        self.first = first\n",
    "        self.second = second"
   ]
